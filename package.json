{
  "name": "tom-waterton-website",
  "description": "Website for Tom Waterton",
  "version": "5.0.0",
  "author": "Tom Waterton",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "gatsby": "^5.15.0",
    "gatsby-cli": "^5.14.0",
    "gatsby-plugin-canonical-urls": "^5.15.0",
    "gatsby-plugin-cname": "^1.0.0",
    "gatsby-plugin-google-analytics": "^5.15.0",
    "gatsby-plugin-google-gtag": "^5.15.0",
    "gatsby-plugin-manifest": "^5.15.0",
    "gatsby-plugin-offline": "^6.15.0",
    "gatsby-plugin-postcss": "^6.15.0",
    "gatsby-plugin-purgecss": "^6.2.1",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-json": "^5.15.0",
    "graphql": "^16.12.0",
    "joi": "^17.13.3",
    "parse-url": "^9.2.0",
    "prop-types": "15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-parser": "^3.3.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "ajv": "^8.17.1",
    "axios": "^1.13.2",
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.20.4",
    "cookie": "^1.1.1",
    "cssnano": "^7.1.2",
    "engine.io": "^6.6.4",
    "eslint": "8.23.1",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "^6.3.0",
    "got": "^13.0.0",
    "minimatch": "^9.0.5",
    "prettier": "^3.7.4",
    "semver": "^7.7.3",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "articles",
    "content",
    "crosswords",
    "design",
    "gatsby",
    "patents",
    "waterton"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomwaterton/website"
  },
  "bugs": {
    "url": "https://github.com/tomwaterton/website/issues"
  }
}
