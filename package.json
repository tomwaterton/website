{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "4.0.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "engine.io": ">=6.2.1",
    "gatsby": "^4.23.0",
    "gatsby-cli": "^4.23.0",
    "gatsby-plugin-cname": "^1.0.0",
    "gatsby-plugin-google-analytics": "^5.1.0",
    "gatsby-plugin-google-gtag": "^4.23.0",
    "gatsby-plugin-manifest": "^4.23.0",
    "gatsby-plugin-offline": "5.23.0",
    "gatsby-plugin-postcss": "5.23.0",
    "gatsby-plugin-purgecss": "6.1.2",
    "gatsby-source-filesystem": "^4.23.0",
    "gatsby-transformer-json": "^4.23.0",
    "got": ">=11.8.5",
    "graphql": "^15.8.0",
    "joi": "^17.6.0",
    "minimatch": ">=3.0.5",
    "parse-url": "^8.1.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "babel-eslint": "10.0.3",
    "cssnano": "^5.1.13",
    "engine.io": ">=6.2.1",
    "eslint": "8.23.1",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "^4.0.0",
    "got": ">=11.8.5",
    "minimatch": ">=3.0.5",
    "prettier": "2.7.1",
    "sharp": "^0.31.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomwaterton/tomwaterton.git"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
}
